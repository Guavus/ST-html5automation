<?xml version="1.0"?>
<data>


    <screenConfigs>
    <screenConfig id = "mrxUserPopUp_Screen">
            <component id = "allinputs" selector="css selector" locator="gvs-popup.add-edit-user-popup" parent="True"/>
            <component id = "allcheckboxes" selector="css selector" locator="gvs-popup.add-edit-user-popup" parent="True"/>
            <component id ="allbuttons" selector="css selector" locator="gvs-popup.add-edit-user-popup" parent="True"/>
            <component id ="allselects" selector="css selector" locator="gvs-popup.add-edit-user-popup" parent="True"/>
            <component id ="icons" selector="css selector" locator="gvs-popup.add-edit-user-popup" parent="True"/>
            <component id ="allsliders" selector="css selector" locator="gvs-popup.add-edit-user-popup" parent="True"/>
            <component id="errorMsgContainer" selector="css selector" locator="gvs-popup.add-edit-user-popup" parent="True"/>
